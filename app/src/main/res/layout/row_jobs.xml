<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/activity_bg">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginBottom="@dimen/_3sdp"
        android:layout_marginTop="@dimen/_5sdp"
        >


        <com.google.android.material.card.MaterialCardView
            android:id="@+id/track_card"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:cardCornerRadius="@dimen/_10sdp"
            app:cardElevation="0dp"
            android:elevation="0dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="@dimen/_9sdp"
            android:layout_marginBottom="@dimen/_1sdp"
            android:layout_marginStart="@dimen/_13sdp"
            android:layout_marginEnd="@dimen/_13sdp"
            app:strokeWidth="0.7dp"
            app:strokeColor="@color/job_stroke_color"
            app:cardBackgroundColor="@color/white"
            >
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/cl_parentcl"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:paddingStart="@dimen/_11sdp"
                android:paddingEnd="@dimen/_11sdp"
                android:paddingBottom="@dimen/_11sdp"
                android:paddingTop="@dimen/_11sdp"
                >
                <ImageView
                    android:id="@+id/iv_stamp"
                    android:layout_width="@dimen/_80sdp"
                    android:layout_height="@dimen/_80sdp"
                    android:src="@drawable/ic_complete_stamp"
                    app:layout_constraintBottom_toTopOf="@id/ll_address"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginTop="@dimen/_10sdp"
                    app:layout_constraintTop_toBottomOf="@id/ll_jobinfo"
                    android:visibility="visible"
                    />
                <TextView
                    android:id="@+id/tv_servicetime"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toTopOf="@+id/tv_trackOrder"
                    app:layout_constraintBottom_toBottomOf="@+id/tv_trackOrder"
                    app:layout_constraintStart_toStartOf="parent"
                    android:text="Time"
                    android:textColor="@color/color_344054"
                    android:textSize="@dimen/_10ssp"
                    android:fontFamily="@font/helvetica"
                    />
                <TextView
                    android:id="@+id/tv_trackOrder"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:text="  Dispached"
                    android:drawableStart="@drawable/ic_right_dispatch"
                    android:drawableTint="@color/color_primary"
                    android:layout_marginTop="0dp"
                    android:textColor="@color/colorPrimaryDark"
                    android:textSize="@dimen/_9ssp"
                    android:background="@drawable/bk_call"
                    android:backgroundTint="@color/color_primary_light"
                    android:fontFamily="@font/sfprodisplay_medium"
                    android:paddingStart="@dimen/_11sdp"
                    android:paddingEnd="@dimen/_11sdp"
                    android:paddingTop="@dimen/_4sdp"
                    android:paddingBottom="@dimen/_5sdp"
                    android:gravity="center"
                    />

                <TextView
                    android:id="@+id/tv_help"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toTopOf="@id/tv_name"
                    app:layout_constraintBottom_toBottomOf="@id/tv_name"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:text="  Help"
                    android:textColor="@color/color_344054"
                    android:textSize="@dimen/_12ssp"
                    android:paddingTop="@dimen/_3sdp"
                    android:paddingBottom="@dimen/_3sdp"
                    android:fontFamily="@font/inter_medium"
                    android:drawableStart="@drawable/ic_disclaimer"
                    android:gravity="center"
                    android:drawableTint="@color/orange"
                    />

                <TextView
                    android:id="@+id/tv_name"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toBottomOf="@id/tv_servicetime"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toStartOf="@id/tv_trackOrder"
                    android:text="Name"
                    android:textColor="@color/color181818"
                    android:textSize="@dimen/_14ssp"
                    android:layout_marginTop="@dimen/_6sdp"
                    android:layout_marginEnd="@dimen/_5sdp"
                    android:singleLine="true"
                    android:ellipsize="end"
                    android:fontFamily="@font/sfprodisplay_bold"
                    />



                <LinearLayout
                    android:id="@+id/ll1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toBottomOf="@id/tv_name"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:orientation="horizontal"
                    android:weightSum="3">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:layout_marginEnd="@dimen/_6sdp"
                        >
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Order No."
                            android:singleLine="true"
                            android:textColor="#66000000"
                            android:textSize="@dimen/_9ssp"
                            android:layout_marginTop="@dimen/_2sdp"
                            />
                        <TextView
                            android:id="@+id/tv_orderno"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="-"
                            android:singleLine="true"
                            android:textColor="@color/color_344054"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:textSize="@dimen/_11ssp"
                            android:fontFamily="@font/inter_bold"
                            />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:layout_marginStart="@dimen/_3sdp"
                        android:layout_marginEnd="@dimen/_3sdp"
                        >
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Size"
                            android:textColor="#66000000"
                            android:singleLine="true"
                            android:textSize="@dimen/_9ssp"
                            android:layout_marginTop="@dimen/_2sdp"
                            />
                        <TextView
                            android:id="@+id/tv_unit"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text=""
                            android:textColor="@color/color_344054"
                            android:textSize="@dimen/_11ssp"
                            android:singleLine="true"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:fontFamily="@font/inter_bold"
                            />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:layout_marginStart="@dimen/_6sdp"
                        >
                        <TextView

                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Amount"
                            android:singleLine="true"
                            android:textColor="#66000000"
                            android:textSize="@dimen/_9ssp"
                            android:layout_marginTop="@dimen/_2sdp"
                            />
                        <TextView
                            android:id="@+id/tv_orderCost"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="-"
                            android:singleLine="true"
                            android:textColor="@color/color_344054"
                            android:textSize="@dimen/_11ssp"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:fontFamily="@font/inter_bold"
                            />
                    </LinearLayout>
                </LinearLayout>



                <LinearLayout
                    android:id="@+id/ll_orderno"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toBottomOf="@id/ll1"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginTop="@dimen/_2sdp"
                    android:visibility="gone"
                    android:weightSum="3">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toBottomOf="@id/tv_name"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:text="Order No."
                        android:textColor="#66000000"
                        android:textSize="@dimen/_9ssp"
                        android:layout_marginTop="@dimen/_2sdp"
                        android:fontFamily="@font/plussakartasans_medium"
                        android:layout_weight="2.1"
                        />
                    <TextView
                        android:layout_width="@dimen/_2sdp"
                        android:layout_height="wrap_content"
                        android:text=":"
                        android:gravity="center"
                        android:layout_marginTop="@dimen/_2sdp"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toBottomOf="@id/tv_name"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:text="-"
                        android:textColor="@color/black"
                        android:textSize="@dimen/_10ssp"
                        android:layout_marginTop="@dimen/_2sdp"
                        android:fontFamily="@font/plusjakartasans_semi_bold"
                        android:layout_marginStart="@dimen/_3sdp"
                        android:layout_weight="0.9"
                        />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_jobinfo"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toBottomOf="@id/ll_orderno"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:weightSum="3">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toBottomOf="@id/tv_name"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:text="Service Name"
                        android:textColor="#66000000"
                        android:textSize="@dimen/_9ssp"
                        android:layout_marginTop="@dimen/_2sdp"
                        android:fontFamily="@font/plussakartasans_medium"
                        android:layout_weight="2.1"
                        />
                    <TextView
                        android:layout_width="@dimen/_2sdp"
                        android:layout_height="wrap_content"
                        android:text=":"
                        android:gravity="center"
                        android:layout_marginTop="@dimen/_2sdp"/>
                    <TextView
                        android:id="@+id/tv_jobinfo"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toBottomOf="@id/tv_name"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:text="-"
                        android:textColor="@color/colorPrimaryDark"
                        android:textSize="@dimen/_11ssp"
                        android:layout_marginTop="@dimen/_2sdp"
                        android:fontFamily="@font/sfprodisplay_medium"
                        android:layout_marginStart="@dimen/_3sdp"
                        android:layout_weight="0.9"
                        />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_categogy"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toBottomOf="@id/ll_jobinfo"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:weightSum="3">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toBottomOf="@id/tv_name"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:text="Category"
                        android:textColor="#66000000"
                        android:textSize="@dimen/_9ssp"
                        android:layout_marginTop="@dimen/_2sdp"
                        android:fontFamily="@font/plussakartasans_medium"
                        android:layout_weight="2.1"
                        />
                    <TextView
                        android:layout_width="@dimen/_2sdp"
                        android:layout_height="wrap_content"
                        android:text=":"
                        android:gravity="center"
                        android:layout_marginTop="@dimen/_2sdp"/>
                    <TextView
                        android:id="@+id/tv_category"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toBottomOf="@id/tv_name"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:text="-"
                        android:textColor="@color/black"
                        android:textSize="@dimen/_10ssp"
                        android:layout_marginTop="@dimen/_2sdp"
                        android:fontFamily="@font/sfprodisplay_medium"
                        android:layout_marginStart="@dimen/_3sdp"
                        android:layout_weight="0.9"
                        />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_subcategory"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toBottomOf="@id/ll_categogy"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:weightSum="3">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toBottomOf="@id/tv_name"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:text="Subcategory"
                        android:textColor="#66000000"
                        android:textSize="@dimen/_9ssp"
                        android:layout_marginTop="@dimen/_2sdp"
                        android:fontFamily="@font/plussakartasans_medium"
                        android:layout_weight="2.1"
                        />
                    <TextView
                        android:layout_width="@dimen/_2sdp"
                        android:layout_height="wrap_content"
                        android:text=":"
                        android:gravity="center"
                        android:layout_marginTop="@dimen/_2sdp"/>
                    <TextView
                        android:id="@+id/tv_subcategory"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toBottomOf="@id/tv_name"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:text="-"
                        android:textColor="@color/black"
                        android:textSize="@dimen/_10ssp"
                        android:layout_marginTop="@dimen/_2sdp"
                        android:fontFamily="@font/sfprodisplay_medium"
                        android:layout_marginStart="@dimen/_3sdp"
                        android:layout_weight="0.9"
                        />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_servicePlan"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toBottomOf="@id/ll_subcategory"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginTop="@dimen/_2sdp"
                    android:visibility="gone"
                    android:weightSum="3">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toBottomOf="@id/tv_name"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:text="Service Plan"
                        android:textColor="#66000000"
                        android:textSize="@dimen/_9ssp"
                        android:layout_marginTop="@dimen/_2sdp"
                        android:fontFamily="@font/plussakartasans_medium"
                        android:layout_weight="2.1"
                        />
                    <TextView
                        android:layout_width="@dimen/_2sdp"
                        android:layout_height="wrap_content"
                        android:text=":"
                        android:gravity="center"
                        android:layout_marginTop="@dimen/_2sdp"/>
                    <TextView
                        android:id="@+id/tv_serviceplan"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toBottomOf="@id/tv_name"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:text="Service Plan"
                        android:layout_marginStart="@dimen/_3sdp"
                        android:textColor="@color/black"
                        android:textSize="@dimen/_9ssp"
                        android:layout_marginTop="@dimen/_2sdp"
                        android:fontFamily="@font/plusjakartasans_semi_bold"
                        android:layout_weight="0.9"
                        />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_type"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toBottomOf="@id/ll_servicePlan"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginTop="@dimen/_2sdp"
                    android:visibility="visible"
                    android:weightSum="3">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toBottomOf="@id/tv_name"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:text="Service Type"
                        android:textColor="#66000000"
                        android:textSize="@dimen/_9ssp"
                        android:layout_marginTop="@dimen/_2sdp"
                        android:fontFamily="@font/plussakartasans_medium"
                        android:layout_weight="2.1"
                        />
                    <TextView
                        android:layout_width="@dimen/_2sdp"
                        android:layout_height="wrap_content"
                        android:text=":"
                        android:gravity="center"
                        android:layout_marginTop="@dimen/_2sdp"/>
                    <TextView
                        android:id="@+id/tv_service_type"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toBottomOf="@id/tv_name"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:text=""
                        android:layout_marginStart="@dimen/_3sdp"
                        android:textColor="@color/black"
                        android:textSize="@dimen/_9ssp"
                        android:layout_marginTop="@dimen/_2sdp"
                        android:fontFamily="@font/plusjakartasans_semi_bold"
                        android:layout_weight="0.9"
                        />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_propertysize"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toBottomOf="@id/ll_type"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginTop="@dimen/_2sdp"
                    android:weightSum="3"
                    android:visibility="gone"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toBottomOf="@id/tv_name"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:text="Property Size"
                        android:textColor="#66000000"
                        android:textSize="@dimen/_9ssp"
                        android:layout_marginTop="@dimen/_2sdp"
                        android:fontFamily="@font/plussakartasans_medium"
                        android:layout_weight="2.1"
                        />
                    <TextView
                        android:layout_width="@dimen/_2sdp"
                        android:layout_height="wrap_content"
                        android:text=":"
                        android:gravity="center"
                        android:layout_marginTop="@dimen/_2sdp"/>

                    <TextView
                        android:id="@+id/tv_propertysize"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toBottomOf="@id/tv_name"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:text="Service Plan"
                        android:layout_marginStart="@dimen/_3sdp"
                        android:textColor="@color/black"
                        android:textSize="@dimen/_9ssp"
                        android:layout_marginTop="@dimen/_2sdp"
                        android:fontFamily="@font/plusjakartasans_semi_bold"
                        android:layout_weight="0.9"
                        />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_address"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toBottomOf="@id/ll_propertysize"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:weightSum="3">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toBottomOf="@id/tv_name"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:text="Address"
                        android:textColor="#66000000"
                        android:textSize="@dimen/_9ssp"
                        android:layout_marginTop="@dimen/_2sdp"
                        android:fontFamily="@font/plussakartasans_medium"
                        android:layout_weight="2.1"
                        />
                    <TextView
                        android:layout_width="@dimen/_2sdp"
                        android:layout_height="wrap_content"
                        android:text=":"
                        android:gravity="center"
                        android:layout_marginTop="@dimen/_2sdp"/>
                    <TextView
                        android:id="@+id/tv_address"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toBottomOf="@id/ll_address"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:text="-"
                        android:textColor="@color/black"
                        android:layout_marginStart="@dimen/_3sdp"
                        android:textSize="@dimen/_10ssp"
                        android:layout_marginTop="@dimen/_2sdp"
                        android:fontFamily="@font/sfprodisplay_medium"
                        android:layout_weight="0.9"
                        />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_collectamount"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toBottomOf="@id/ll_address"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginTop="@dimen/_2sdp"
                    android:visibility="gone"
                    android:weightSum="3">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toBottomOf="@id/tv_name"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:text="Amount to collect"
                        android:textColor="#66000000"
                        android:textSize="@dimen/_9ssp"
                        android:layout_marginTop="@dimen/_2sdp"
                        android:fontFamily="@font/plussakartasans_medium"
                        android:layout_weight="2.1"
                        />
                    <TextView
                        android:layout_width="@dimen/_2sdp"
                        android:layout_height="wrap_content"
                        android:text=":"
                        android:gravity="center"
                        android:layout_marginTop="@dimen/_2sdp"/>
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toBottomOf="@id/ll_address"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:text="0"
                        android:textColor="@color/black"
                        android:layout_marginStart="@dimen/_3sdp"
                        android:textSize="@dimen/_10ssp"
                        android:layout_marginTop="@dimen/_2sdp"
                        android:fontFamily="@font/plusjakartasans_semi_bold"
                        android:layout_weight="0.9"
                        />

                </LinearLayout>

                <View
                    android:id="@+id/view_nav"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="#E8E8E8"
                    android:layout_marginTop="@dimen/_11sdp"
                    app:layout_constraintTop_toBottomOf="@id/ll_collectamount"/>

                <LinearLayout
                    android:id="@+id/cl2"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/_25sdp"
                    app:layout_constraintStart_toStartOf="@+id/tv_name"
                    app:layout_constraintTop_toBottomOf="@id/view_nav"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:layout_marginBottom="@dimen/_2sdp"
                    android:layout_marginTop="@dimen/_11sdp"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:weightSum="4"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:id="@+id/ll_call"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1.2"
                        android:orientation="horizontal"
